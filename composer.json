{
    "name": "mastani/laravel-idpay",
    "description": "IDPay payment gateway for Laravel https://idpay.ir",
	"keywords": [
        "idpay",
        "gateway",
        "payment",
        "laravel"
    ],
    "type": "library",
	"homepage": "https://github.com/mastani/laravel-idpay",
	"license": "MIT",
    "authors": [
        {
            "name": "Amin Mastani",
            "email": "amin.mastani@yahoo.com"
        }
    ],
    "require": {
        "illuminate/contracts": "5.*|6.*|7.*|8.*|^9.0",
        "illuminate/support": "5.*|6.*|7.*|8.*|^9.0"
	},
    "require-dev": {
        "phpunit/phpunit" : "9.*"
    },
	"autoload": {
        "psr-4": {
            "mastani\\IDPay\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mastani\\IDPay\\IDPayServiceProvider"
            ],
            "aliases": {
                "IDPay": "mastani\\IDPay\\IDPayFacade"
            }
        }
    }
}
